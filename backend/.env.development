# Development Environment Configuration
NODE_ENV=development
PORT=5001
ENV_NAME=Development

# Database (Development)
MONGODB_URI=mongodb://localhost:27017/nexttechfusion_dev
DB_NAME=nexttechfusion_dev

# JWT Configuration (Development)
JWT_SECRET=dev-super-secret-jwt-key-for-development-only
JWT_EXPIRE=7d
JWT_REFRESH_SECRET=dev-refresh-secret-key

# Redis Configuration (Development)
REDIS_URL=redis://localhost:6379/0

# Email Configuration (Development)
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USER=dev-email@gmail.com
EMAIL_PASS=dev-app-password

# Payment Configuration (Development - Test Keys)
# Stripe
STRIPE_SECRET_KEY=sk_test_dev_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_dev_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_dev_webhook_secret

# Razorpay
RAZORPAY_KEY_ID=rzp_test_dev_key_id
RAZORPAY_KEY_SECRET=dev_razorpay_secret

# PayPal
PAYPAL_CLIENT_ID=dev_paypal_client_id
PAYPAL_CLIENT_SECRET=dev_paypal_client_secret
PAYPAL_MODE=sandbox

# UPI Configuration
UPI_MERCHANT_ID=dev_merchant_id
UPI_MERCHANT_KEY=dev_merchant_key

# Google Pay
GOOGLE_PAY_MERCHANT_ID=dev_google_pay_merchant_id
GOOGLE_PAY_ENVIRONMENT=TEST

# AI Configuration
OPENAI_API_KEY=dev-openai-api-key

# Frontend URL
FRONTEND_URL=http://localhost:3000

# File Upload Configuration (Development)
CLOUDINARY_CLOUD_NAME=dev-cloudinary-name
CLOUDINARY_API_KEY=dev-cloudinary-key
CLOUDINARY_API_SECRET=dev-cloudinary-secret

# Security (Development)
BCRYPT_ROUNDS=10
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Monitoring (Development)
ENABLE_LOGGING=true
LOG_LEVEL=info
ENABLE_DETAILED_LOGS=false

# Development Features
ENABLE_CORS=true
ENABLE_MOCK_DATA=true
ENABLE_DEBUG_ROUTES=false